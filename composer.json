{
    "name": "MKRazz/blade-extensions",
    "description": "Laravel package providing additional Blade extensions: foreach (with $loop data like twig), break, continue, set,array (multiline), etc",
    "homepage": "https://github.com/MKRazz/blade-extensions",
    "keywords": [
        "loop",
        "extensions",
        "blade",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "MKRazz/support": "1.0.*"
    },
    "repositories": {
        "support": {
            "type": "package",
            "package": {
                "name": "MKRazz/support",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/MKRazz/support.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    },
    "suggest": {
        "raveren/kint": ">0.9.1",
        "illuminate/html": "~5.0",
        "erusev/parsedown": "~1.5"
    },
    "require-dev": {
        "laradic/dev": "dev-master",
        "raveren/kint": "0.9.*",
        "illuminate/html": "~5.0",
        "erusev/parsedown": "~1.5",
        "kzykhys/ciconia": "1.0.*"
    },
    "scripts": {
    },
    "autoload": {
        "psr-4": {
            "Radic\\BladeExtensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radic\\Tests\\BladeExtensions\\": "tests/"
        }
    }
}
